{
    "name": "beapp/bugsnag-symfony-ext",
    "description": "Extensions for Bugsnag Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "bugsnag",
        "symfony",
        "extensions"
    ],
    "authors": [
        {
            "name": "Damien Villeneuve",
            "email": "d.villeneuve@beapp.fr"
        }
    ],
    "require": {
        "php": ">=8.0",
        "bugsnag/bugsnag-symfony": "^1.8",
        "symfony/http-foundation": "^5|^6|^7"
    },
    "autoload": {
        "psr-4": {
            "Beapp\\Bugsnag\\Ext\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beapp\\Bugsnag\\Ext\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
